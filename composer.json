{
    "name": "creitive/laravel-breadcrumbs",
    "description": "Breadcrumbs integration for Laravel 7+",
    "keywords":[
        "laravel",
        "breadcrumbs"
    ],
    "homepage":"https://github.com/creitive/laravel-breadcrumbs",
    "authors": [
        {
            "name": "Milos Levacic",
            "email": "milos.levacic@creitive.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "creitive/breadcrumbs": "^3.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Creitive\\Breadcrumbs\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creitive\\Breadcrumbs\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "Creitive\\Breadcrumbs\\Facades\\Breadcrumbs"
            }
        }
    },
    "minimum-stability": "dev"
}
